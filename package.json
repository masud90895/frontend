  "scripts": {
    "reset": " rimraf *.lock && rimraf node_modules/ && metafox bootstrap",
    "bootstrap": "metafox bootstrap",
    "test": "jest",
    "start": "PORT=3000 metafox start --type=web",
    "start:admincp": "PORT=3001 metafox start --type=admincp --profile=admincp",
    "start:installation": "PORT=3003 metafox start --profile=installation --type=installation",
    "start:dev0": "nodemon --watch scripts -e js scripts/server.js",
    "bundle": "pnpm i --no-frozen-lockfile && metafox bootstrap && metafox build --production",
    "bundle:parallel": "pnpm i --prefer-frozen-lockfile && concurrently 'npm run bundle:web' 'npm run bundle:admincp'",
    "bundle:release": "pnpm i --prefer-frozen-lockfile && metafox bootstrap && metafox build --profile=release --production",
    "bundle:web": "metafox build:process --type=web --production",
    "bundle:admincp": "metafox build:process --type=admincp --production",
    "build:installation": "NODE_ENV=production metafox build:process --profile=installation --type=installation --production",
    "build:call": "NODE_ENV=production metafox build:process --profile=call --type=call --production",
    "build:multi-target": "metafox build --profile=multi-target --production",
    "build:preview": "metafox build --profile=preview --production",
    "build:team": "metafox build --profile=team --production",
    "export": "metafox export",
    "fix:layout": "npx ts-node scripts/fix-layout.ts && metafox reload",
    "reload": "metafox reload",
    "reload:admincp": "metafox reload --profile=admincp --type=admincp",
    "reload:installation": "metafox reload --profile=installation --type=installation",
    "publish:installation": "metafox publish --type=installation --profile=installation --production",
    "package:list": "pnpm m ls --json --depth -1",
    "unittest": "metafox jest-unittest",
    "list-deps": "pnpm m list --json --depth -1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ",
