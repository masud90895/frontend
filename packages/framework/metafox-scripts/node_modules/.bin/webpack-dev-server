#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/masud90895/Downloads/frontend/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack@5.102.1_@swc+core@1.13.5_esbuild@0.27.0_/node_modules/webpack-dev-server/bin/node_modules:/Users/masud90895/Downloads/frontend/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack@5.102.1_@swc+core@1.13.5_esbuild@0.27.0_/node_modules/webpack-dev-server/node_modules:/Users/masud90895/Downloads/frontend/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack@5.102.1_@swc+core@1.13.5_esbuild@0.27.0_/node_modules:/Users/masud90895/Downloads/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/masud90895/Downloads/frontend/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack@5.102.1_@swc+core@1.13.5_esbuild@0.27.0_/node_modules/webpack-dev-server/bin/node_modules:/Users/masud90895/Downloads/frontend/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack@5.102.1_@swc+core@1.13.5_esbuild@0.27.0_/node_modules/webpack-dev-server/node_modules:/Users/masud90895/Downloads/frontend/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack@5.102.1_@swc+core@1.13.5_esbuild@0.27.0_/node_modules:/Users/masud90895/Downloads/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
